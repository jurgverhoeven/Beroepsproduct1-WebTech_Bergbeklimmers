diff --git a/NavTest/CSS/Style.css b/NavTest/CSS/Style.css
new file mode 100644
index 0000000..90488a9
--- /dev/null
+++ b/NavTest/CSS/Style.css
@@ -0,0 +1,106 @@
+* {
+    margin: 0px;
+    padding: 0px;
+    box-sizing: border-box;
+    font-family: Arial;
+}
+
+header{
+    background-color: #333;
+    color: white;
+    padding: 10px;
+    width: 100%;
+}
+
+
+header nav ul{
+    list-style-type: none;
+    margin: 0;
+    padding: 0;
+    overflow: hidden;
+    font-weight: bold;
+}
+
+header nav ul li{
+    float: left;
+}
+
+nav ul li a{
+    display: block;
+    color: white;
+    text-align: center;
+    padding: 14px 16px;
+    text-decoration: none;
+}
+
+header nav ul li:hover{
+    background-color: #111;
+    text-decoration: underline;
+}
+
+aside nav ul{
+    list-style-type: none;
+    margin: 0;
+    padding: 0;
+    width: 25%;
+    background-color: #333;
+    position: fixed;
+    height: 100%;
+    overflow: auto;
+    font-weight: bold;
+}
+
+aside nav ul li a{
+    display: block;
+    color: white;
+    padding: 14px 16px;
+    text-decoration: none;
+    background-image: linear-gradient(#333333, #3c3c3c);
+}
+
+aside nav ul li a:hover{
+    background-image: none;
+    background-color: #111;
+    color: white;
+    text-decoration: underline;
+}
+
+@media only screen and (max-width: 960px) {
+    header nav{
+        display: none;
+    }
+}
+
+@media only screen and (min-width: 961px){
+    aside{
+        display: none;
+    }
+}
+
+.dropdown-menu{
+    display: none;
+    position: absolute;
+    background-color: #f9f9f9;
+    box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
+    z-index: 1;
+}
+
+.dropdown:hover .dropdown-menu{
+    display: block;
+}
+
+li.dropdown {
+    display: block;
+}
+
+.dropdown-menu a {
+    color: black;
+    padding: 14px 16px;
+    text-decoration: none;
+    display: block;
+    text-align: left;
+}
+
+.dropdown-menu a:hover{
+    text-decoration: underline;
+}
